    "horizontalMenuToggle": "hover",
    "horizontalMenuAnimation": true,
    "appBar": "fixed",
    "appBarBlur": true,
    "responsiveFontSizes": true,
    "disableRipple": false,
    "disableCustomizer": true,
    "toastPosition": "top-right",
    "primaryMain": "#ea5455",
    "primaryLight": "#ea5455",
    "primaryDark": "#ea5455"
