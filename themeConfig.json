{
    "templateName": "Numed Diagnostics",
    "logo": "/images/logo.png",
    "layout": "horizontal",
    "mode": "light",
    "direction": "ltr",
    "skin": "default",
    "contentWidth": "full",
    "appBarVariant": "v2",
    "footer": "static",
    "routingLoader": true,
    "navHidden": false,
    "menuTextTruncate": true,
    "navSubItemIcon": "tabler:circle",
